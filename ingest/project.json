{
  "name": "ingest",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./ingest",
  "targets": {
    "build": {
    "executor": "nx:run-commands",
      "options": {
        "cwd": "ingest/",
        "command": "./build.sh"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./dist/ingest"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    }
  },
  "tags": []
}
